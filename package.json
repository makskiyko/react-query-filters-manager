{
  "name": "react-query-filters-manager",
  "version": "1.0.2",
  "description": "react-query-filters-manager",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir  dist/cjs --allowSyntheticDefaultImports",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makskiyko/react-query-filters-manager.git"
  },
  "keywords": [
    "react query",
    "filters manager",
    "filters"
  ],
  "author": "makskiyko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makskiyko/react-query-filters-manager/issues"
  },
  "homepage": "https://github.com/makskiyko/react-query-filters-manager#readme",
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.22.3",
    "next": "^13.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
